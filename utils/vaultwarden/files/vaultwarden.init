#!/bin/sh /etc/rc.common
# Copyright (C) 2009-2023 OpenWrt.org

START=99
STOP=80

USE_PROCD=1

VW_BASE="/etc/vaultwarden"
VW_BIN="/usr/bin/vaultwarden"

start_service() {
    procd_open_instance
    procd_set_param respawn
    procd_set_param stdout 1
    procd_set_param stderr 1
    procd_set_param command sh -c "cd $VW_BASE; $VW_BIN"
    procd_close_instance
}
